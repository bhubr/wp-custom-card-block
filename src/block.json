{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/wp-custom-card-block",
	"version": "0.1.0",
	"title": "Wp Custom Card Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "wp-custom-card-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h3"
		},
		"description": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"pictureUrl": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"bgColor": {
			"type": "string",
			"source": "attribute",
			"selector": "div.square",
			"attribute": "data-bgColor"
		}
	}
}
